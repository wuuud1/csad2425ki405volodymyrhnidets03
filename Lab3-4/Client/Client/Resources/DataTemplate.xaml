<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Client.Presentation.ViewModels"
                    xmlns:v="clr-namespace:Client.Presentation.Views">

    <DataTemplate DataType="{x:Type TypeName=vm:HomeViewModel}">
        <v:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=vm:SettingsViewModel}">
        <v:Settings/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=vm:GameViewModel}">
        <v:Game/>
    </DataTemplate>
</ResourceDictionary>